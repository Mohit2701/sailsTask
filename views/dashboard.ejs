
<%- partial('js/registration') %>
<div class="container">
<table class="table">
    <thead>
        <tr>
            <th>
                <input type="text" id="FirstName" placeholder="Seach First Name">
            </th>
            <th>
                <input type="text" id="LastName" placeholder="Seach Last Name">
            </th>
            <th>
                <input type="text" id="Email" placeholder="Seach Email ID">
            </th>
            <th>
                
            </th>
            <th>
                
            </th>
        </tr>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email ID</th>
            <th>Employee ID</th>
            <th>Organization</th>
        </tr>
        
    </thead>
    <tbody>
        <%data.forEach(function (Emp) { %>
            <tr>
                <td><%=Emp.UserId.FirstName%></td>
                <td><%=Emp.UserId.LastName%></td>
                <td><%=Emp.UserId.Email%></td>
                <td><%=Emp.EmpID%></td>
                <td><%=Emp.Organization%></td>
            </tr>
        <%});%>
        
    </tbody>
</table>
   
</div>
<script>
    $(document).ready(function () {
         var table = $('.table').DataTable();

        $('#FirstName').on('keyup', function () {
            table
                .column(0)
                .search(this.value)
                .draw();
        });
        $('#LastName').on('keyup', function () {
            table
                .column(1)
                .search(this.value)
                .draw();
        });
        $('#Email').on('keyup', function () {
            table
                .column(2)
                .search(this.value)
                .draw();
        });
    });
</script>
